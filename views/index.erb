<html>
  <head>
    <title>FeedMe!</title>
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
  </head>
  <body>
    <div class="container">
      <h1>FeedMe!<small> Got <%= Feed.all.count %> feeds in db..</small</h1>
        <table class="table table-hover">
            <% @feeds.each do |feed| %>
          <tr>
            <td><%= feed.category %></td>
            <td><a href="<%= feed.url %>"><%= feed.title %></a></td>
          </tr>
            <% end%>
        </table>
    </div>
    </div>
  </body>
</html>
